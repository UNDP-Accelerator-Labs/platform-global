						.replace(/Mar/i, 'março')
						.replace(/Apr/i, 'abril')
						.replace(/May/i, 'maio')
						.replace(/Jun/i, 'junho')
						.replace(/Jul/i, 'julho')
						.replace(/Aug/i, 'agosto')
						.replace(/Sep/i, 'set.')
						.replace(/Oct/i, 'out.')
						.replace(/Nov/i, 'nov.')
						.replace(/Dec/i, 'dez.')

					if (interval === 'positive') {
						if (type === 'start') return 'começa em ' + date
						else if (type === 'end') return 'termina em ' + date
					} else if (interval === 'negative') {
						if (type === 'start') return 'começou em ' + date
						else if (type === 'end') return 'terminou em ' + date
						else return date
					} else return date
				} else {
					if (days >= 1) {
						if (interval === 'positive') {
							if (type === 'start') {
								if (days === 1) return 'começa em ' + days + ' dia'
								else return 'começa em ' + days + ' dias'
							} else if (type === 'end') {
								if (days === 1) return 'acaba em ' + days + ' dia'
								else return 'acaba em ' + days + ' dias'
							}
						} else if (interval === 'negative') {
							if (type === 'start') {
								if (days === 1) return 'começou há ' +  days + ' dia'
								else return 'começou há ' + days + ' dias'
							} else if (type === 'end') {
								if (days === 1) return 'terminou há ' +  days + ' dia'
								else return 'terminou há ' + days + ' dias'
							} else {
								if (days === 1) return days + ' dia atrás'
								else return days + ' dias atrás'
							}
						}
					} else {
						if (hours >= 1) {
							if (interval === 'positive') {
								if (type === 'start') {
									if (hours === 1) return 'começa em ' + hours + ' hora'
									else return 'começa em ' + hours + ' horas'
								} else if (type === 'end') {
									if (hours === 1) return 'acaba em ' + hours + ' hora'
									else return 'acaba em ' + hours + ' horas'
								}
							} else if (interval === 'negative') {
								if (type === 'start') {
									if (hours === 1) return 'começou há ' + hours + ' hora'
									else return 'começou há ' + hours + ' horas'
								} else if (type === 'end') {
									if (hours === 1) return 'terminou há ' + hours + ' hora'
									else return 'terminou há ' + hours + ' horas'
								} else {
									if (hours === 1) return hours + ' hora atrás'
									else return hours + ' horas atrás'
								}
							}
						}
						else {
							if (interval === 'positive') {
								if (type === 'start') {
									if (minutes === 1) return 'começa em ' + minutes + ' minuto'
									else return 'começa em ' + minutes + ' minutos'
								} else if (type === 'end') {
									if (minutes === 1) return 'acaba em ' + minutes + ' minuto'
									else return 'acaba em ' + minutes + ' minutos'
								}
							} else if (interval === 'negative') {
								if (type === 'start') {
									if (minutes === 1) return 'começou há ' + minutes + ' minuto'
									else return 'começou há ' + minutes + ' minutos'
								} else if (type === 'end') {
