{
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["google", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "camelcase": "off",
    "max-len": "off",
    "require-jsdoc": "off"
  },
  "plugins": ["ejs-js"],
  "ignorePatterns": ["/public/*", "/routes/*", "/views/*"]
}
